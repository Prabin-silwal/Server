{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@now/node-server"
        }
    ],
    "functions": {
        "api/**/*": {
            "memory": 512,
            "runtime": "node@14.x"
        }
    },
    "routes": [
        {
            "src": "/api/scan",
            "dest": "/api/scan"
        },
        {
            "src": "/api/uploadToCloud",
            "dest": "/api/uploadToCloud"
        },
        {
            "src": "/api/displayCloudData",
            "dest": "/api/displayCloudData"
        },
        {
            "src": "/api/delete",
            "dest": "/api/delete"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}